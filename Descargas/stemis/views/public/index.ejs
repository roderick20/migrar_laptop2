<section>
  <style>
    #offer-message {
      position: fixed;
      top: 0;
      left: 0;
      width: 100%;
      height: 100%;
      background: rgba(0, 0, 0, 0.7);
      display: none;
      justify-content: center;
      align-items: center;
      z-index: 9999;
    }

    #offer-content {
      
      box-shadow: 0 0 5px rgba(0, 0, 0, 0);
      position: relative;
      text-align: center;
    }

    #offer-content img {

      height: auto;
      display: block; /* Evita espacio extra debajo de la imagen */
      margin: 0 auto; /* Centra la imagen */
    }

    #close-offer {
      position: absolute;
      top: 0;
      right: 0;
    
      cursor: pointer;
      font-size: 24px;
      color: #ffffff;
      border-radius: 100%;
      background-color: transparent; /* Sin fondo */
      border: none; /* Sin borde */
    }
  </style>
  <div id="offer-message">
    <div id="offer-content">
      <img src="/assets/img/BLOGBUSCADOR/banner2.png" class="rounded-4 w-50" alt="Oferta especial" />
      <button class="BTN" id="close-offer"></button>
    </div>
    <script>
      document.addEventListener('DOMContentLoaded', function () {
        const offerMessage = document.getElementById('offer-message');
        const closeButton = document.getElementById('close-offer');

        setTimeout(function () {
          offerMessage.style.display = 'flex';
        }, 10);

        closeButton.addEventListener('click', function () {
          closeOffer();
        });

        offerMessage.addEventListener('click', function (event) {
          if (event.target === offerMessage) {
            closeOffer();
          }
        });

        function closeOffer() {
          offerMessage.style.display = 'none';
        }
      });
    </script>
  </div>
</section>
