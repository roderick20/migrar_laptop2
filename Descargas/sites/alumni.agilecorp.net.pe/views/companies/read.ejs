<div class="col-12">
  <div class="card border rounded-3">
    <div class="card-header  border-bottom p-3">
      <div class="d-sm-flex justify-content-between align-items-center">
        <h5 class="mb-2 mb-sm-0">Detalle compa√±ia</h5>
      </div>
    </div>
    <div class="card-body">
      <div class="row">
        <% if (error != '') { %>
        <div class="text-danger"><%= error %>"</div>
        <% } else { %>
          
        <dl class="row">
          <dt class="col-sm-2"> Name </dt>
          <dd class="col-sm-10"> <%= company.Name %> </dd>
        </dl>
        <dl class="row">
          <dt class="col-sm-2"> Whatsapp </dt>
          <dd class="col-sm-10"> <%= company.Whatsapp %> </dd>
        </dl>
        <dl class="row">
          <dt class="col-sm-2"> Instagram </dt>
          <dd class="col-sm-10"> <%= company.Instagram %> </dd>
        </dl>
        <dl class="row">
          <dt class="col-sm-2"> Telegram </dt>
          <dd class="col-sm-10"> <%= company.Telegram %> </dd>
        </dl>
        <dl class="row">
          <dt class="col-sm-2"> Facebook </dt>
          <dd class="col-sm-10"> <%= company.Facebook %> </dd>
        </dl>
        <dl class="row">
          <dt class="col-sm-2"> Youtube </dt>
          <dd class="col-sm-10"> <%= company.Youtube %> </dd>
        </dl>
        <dl class="row">
          <dt class="col-sm-2"> TikTok </dt>
          <dd class="col-sm-10"> <%= company.TikTok %> </dd>
        </dl>
        <dl class="row">
          <dt class="col-sm-2"> Twitter </dt>
          <dd class="col-sm-10"> <%= company.Twitter %> </dd>
        </dl>
        <dl class="row">
          <dt class="col-sm-2"> Snapchat </dt>
          <dd class="col-sm-10"> <%= company.Snapchat %> </dd>
        </dl>
        <dl class="row">
          <dt class="col-sm-2"> Spotify </dt>
          <dd class="col-sm-10"> <%= company.Spotify %> </dd>
        </dl>
        <dl class="row">
          <dt class="col-sm-2"> Website </dt>
          <dd class="col-sm-10"> <%= company.Website %> </dd>
        </dl>
        <dl class="row">
          <dt class="col-sm-2"> Linkedin </dt>
          <dd class="col-sm-10"> <%= company.Linkedin %> </dd>
        </dl>
        <dl class="row">
          <dt class="col-sm-2"> Paypal </dt>
          <dd class="col-sm-10"> <%= company.Paypal %> </dd>
        </dl>
        <dl class="row">
          <dt class="col-sm-2"> PhoneNumber </dt>
          <dd class="col-sm-10"> <%= company.PhoneNumber %> </dd>
        </dl>
        <dl class="row">
          <dt class="col-sm-2"> Address </dt>
          <dd class="col-sm-10"> <%= company.Address %> </dd>
        </dl>
        <dl class="row">
          <dt class="col-sm-2"> FaceTime </dt>
          <dd class="col-sm-10"> <%= company.FaceTime %> </dd>
        </dl>
        <dl class="row">
          <dt class="col-sm-2"> Email </dt>
          <dd class="col-sm-10"> <%= company.Email %> </dd>
        </dl>
        <dl class="row">
          <dt class="col-sm-2"> Status </dt>
          <dd class="col-sm-10"> <%= company.Status %> </dd>
        </dl>
        <% } %>
      </div>
    </div>
    <div class="card-footer  border-bottom p-3">
      <div class="d-sm-flex justify-content-between align-items-center">
        <button class="btn btn-sm btn-info" type="button" onclick="window.location.href='/companies'"><i class="fa-solid fa-arrow-left"></i>
          Regresar</button>
        <a class="btn btn-sm btn-primary" asp-action="Edit" asp-route-UniqueId="@Model.UniqueId"><i class="fa-solid fa-pen-to-square"></i> Editar</a>
      </div>
    </div>
  </div>
</div>