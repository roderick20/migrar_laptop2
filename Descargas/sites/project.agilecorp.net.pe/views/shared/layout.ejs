<% if(Object.keys(messages).length !==0) { %>
  <% if(messages.hasOwnProperty('info')) { %>
    <script>
      Swal.fire({
        position: "bottom-end",
        icon: "success",
        title: "<%= messages.info[0] %>",
        showConfirmButton: false,
        timer: 1200
      });
    </script>
    <% } else { %>
      <script>
        Swal.fire({
          position: "bottom-end",
          icon: "error",
          title: "<%= messages.error[0] %>",
          showConfirmButton: false,
          timer: 1200
        });
      </script>
      <% } %>
        <% } %>


